# 데이터베이스 설정
POSTGRES_DB=syncup
POSTGRES_USER=syncup_user
POSTGRES_PASSWORD=syncup_password
POSTGRES_PORT=5433

# 백엔드 설정
BACKEND_PORT=4000
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=7d
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000

# 프론트엔드 설정
FRONTEND_PORT=3000
NEXT_PUBLIC_API_BASE_URL=http://localhost:4000

# Seed 스크립트 실행 여부 (처음 한 번만 true로 설정, 이후 false로 변경)
RUN_SEED=false
